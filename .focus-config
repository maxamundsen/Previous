[13]  # Version number. Do not delete.

[[workspace]]
./

[ignore]
.git
./src/vendor/**
.build
.vscode
*.bak
*.min.js
*.min.css
*.min.js.map
*.min.css.map
./src/wwwroot/lib**
./src/wwwroot/fonts**
*.sqlite
__debug*

[[ settings ]]
tab_size: 4
indent_using:                    tabs  # options: spaces, tabs

[[build commands]]
build_working_dir: ./src
run_working_dir: ./src
error_regex: ^(?P<file>.*):(?P<line>\d+):(?P<col>\d+): (?P<msg>.*)$
clear_build_output_before_running: true

[Debug Build]
build_command: sh build_server.sh debug
key_binding: Cmd-Shift-B
