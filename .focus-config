	[6]  # Version number. Do not delete.

[[workspace]]
./

[ignore]
.git
vendor**
.build
.vscode
*.bak
*.min.js
*.min.css
*.min.js.map
*.min.css.map
lib**
fonts**
*.sqlite
__debug*

[[ settings ]]
tab_size: 4
insert_spaces_when_pressing_tab: false

[[ build commands ]]
build_working_dir: ./src
run_working_dir: ./src
error_regex:                            ^(?P<file>.*):(?P<line>\d+):(?P<col>\d+): (?P<msg>.*)$
clear_build_output_before_running:      true
open_panel_on_build:                    true

[Build Debug Macos]
build_command: /usr/local/go/bin/go build -tags=debug
key_binding:   Cmd-Shift-B

[Fmt]
build_command: /usr/local/go/bin/go fmt ./...
