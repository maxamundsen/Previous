{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Development Website",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src",
            "buildFlags": "-tags=debug",
            "preLaunchTask": "TailwindCSS Compile - MacOS",
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        }
    ]
}