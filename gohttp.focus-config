[2]  # Version number. Do not delete.

[[workspace]]

[workspace dirs]
C:/Projects/GoHttp

[ignore dirs]
.git
vendor

[allow file extensions]

[ignore file extensions]
*min.*

[[ settings ]]
tab_size: 4
insert_spaces_when_pressing_tab: false

[[ build commands ]]
build_working_dir: C:\Projects\GoHttp
run_working_dir: C:\Projects\GoHttp
open_panel_on_build:    true
close_panel_on_success: false

[Build Release]
build_command: go build
key_binding: F5 1

[Build Release (embed)]
build_command: go build -tags embed -o gohttp_embed.exe
key_binding: F5 2

[Debug]
run_command: conhost.exe dlv debug --build-flags "-tags=devel"
open_panel_on_build: false
key_binding: F5 3

[Fmt]
run_command: go fmt ./...
key_binding: F5 4

[Open Terminal (cmd.exe)]
run_command: conhost.exe cmd.exe
key_binding: F5 5
